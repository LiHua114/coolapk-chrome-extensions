{
  "manifest_version": 2,
  "name": "coolapk-web",
  "version": "1.0",
  "description": "provide content for coolapk web",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.coolapk.com/feed/*"],
      "js": ["js/getReply.js"],
      "css": ["css/reply.css"]
    },
    {
      "matches": ["*://www.coolapk.com/apk/*", "*://www.coolapk.com/game/*"],
      "js": ["js/getComment.js"],
      "css": ["css/comment.css"]
    }
  ],
  "permissions": ["*://*.coolapk.com/*"],
  "background": {
    "scripts": ["js/token.js", "js/background.js"]
  },
  "web_accessible_resources": [
    "js/fancybox.js",
    "js/scroll.js",
    "css/fancybox.css"
  ]
}
