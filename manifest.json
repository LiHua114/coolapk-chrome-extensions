{
  "manifest_version": 2,
  "name": "coolapk-web",
  "version": "1.0",
  "description": "provide content for coolapk web",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.coolapk.com/feed/*"],
      "js": ["js/reply.js"],
      "css": ["css/reply.css"]
    },
    {
      "matches": ["*://www.coolapk.com/apk/*", "*://www.coolapk.com/game/*"],
      "js": ["js/comment.js"],
      "css": ["css/comment.css"]
    }
  ],
  "permissions": ["*://*.coolapk.com/*"],
  "background": {
    "scripts": ["js/lib/token.js", "js/background/background.js"]
  },
  "web_accessible_resources": [
    "js/lib/fancybox.js",
    "js/lib/scroll.js",
    "js/lib/toTop.js",
    "js/lib/injectTop.js",
    "css/fancybox.css",
    "img/loader.gif",
    "img/icons.png",
    "img/icons.svg",
    "img/top.png"
  ]
}
