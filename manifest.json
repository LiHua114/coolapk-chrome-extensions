{
  "manifest_version": 2,
  "name": "coolapk-web",
  "version": "1.0",
  "description": "provide content for coolapk web",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.coolapk.com/*"],
      "js": ["js/jquery.js", "js/fancybox.js", "js/demo.js"],
      "css": ["css/fancybox.css", "css/reply.css"]
    }
  ],
  "permissions": ["*://*.coolapk.com/*"],
  "background": {
    "scripts": ["js/token.js", "js/background.js"]
  }
}
